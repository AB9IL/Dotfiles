! xrdb -merge ~/.Xresources (TO APPLY CHANGES)

! Color-scheme : MaterialOcean
! special
*.foreground:   #c6cae0
*.background:   #000000
*.cursorColor:  #c6cae0

! black
*.color0:       #0c0c0c
*.color8:       #767676

! red
*.color1:       #c50f1f
*.color9:       #e74856

! green
*.color2:       #13a10e
*.color10:      #16c60c

! yellow
*.color3:       #c19c00
*.color11:      #f9f1a5

! blue
*.color4:       #82aaff
*.color12:      #3b78ff

! magenta
*.color5:       #881798
*.color13:      #b4009e

! cyan
*.color6:       #3a96dd
*.color14:      #61d6d6

! white
*.color7:       #ccccd0
*.color15:      #f0f0ff

URxvt*termName:  screen-256color
URxvt*scrollBar: false

! Font
URxvt*font:		xft:FiraCode Nerd Font Mono:size=11
URxvt*boldffont:	xft:FiraCode Bold Nerd Font Bold:bold:size=11
URxvt*italicfont:	xft:FiraCode Nerd Font Italic:size=11
URxvt*boldItalicfont:	xft:FiraCode Nerd Font Bold Italic:size=11
URxvt*urlLauncher: x-www-browser
URxvt*matcher.button: 1
URxvt*letterSpace: -1
URxvt*lineSpace: 0

URxvt*cursorBlink: true

URxvt*transparent: true
URxvt*shading: 30

URxvt*iso14755: false
URxvt*iso14755_52: false

URxvt*perl-lib: .urxvt/perl
URxvt*perl-ext-common: default,matcher,font-size,tabbed
URxvt*keysym.C-Right: \033[1;5C
URxvt*keysym.C-Left: \033[1;5D
URxvt*keysym.C-Up: perl:font-size:increase
URxvt*keysym.C-k: perl:font-size:increase
URxvt*keysym.C-Down: perl:font-size:decrease
URxvt*keysym.C-j: perl:font-size:decrease
URxvt*keysym.M-u: perl:url-select:select_next
URxvt*url-select.launcher: x-www-browser
URxvt*url-select.underline: true
URxvt*keysym.M-Escape: perl:keyboard-select:activate
URxvt*keysym.Control-Alt-C: perl:clipboard:copy
URxvt*keysym.Control-Alt-V: perl:clipboard:paste

URxvt*geometry: 120x35+c+c
URxvt*saveLines:  2000

URxvt*tabbed.tabbar-fg: 2
URxvt*tabbed.tabbar-bg: 0
URxvt*tabbed.tab-fg: 3
URxvt*tabbed.tab-bg: 0


UXTerm*termName: xterm-color
UXTerm*scrollBar: false
UXTerm*faceName: FiraCode Nerd Font Mono
UXTerm*faceSize: 11
UXTerm*vt100*geometry: 120x35+c+c
UXTerm*vt100*translations: #override \n\
    Ctrl <Key> minus: smaller-vt-font() \n\
    Ctrl <Key> plus: larger-vt-font() \n\
    Ctrl <Key> 0: set-vt-font(d) \n\
	Shift Ctrl <Key> C: copy-selection(CLIPBOARD) \n\
	Shift Ctrl <Key> V: insert-selection(CLIPBOARD)
UXTerm*saveLines: 2000


xterm*termName: xterm-color
xterm*scrollBar: false
xterm*faceName: FiraCode Nerd Font Mono
xterm*faceSize: 11
xterm*vt100*geometry: 120x35+c+c
xterm*vt100*translations: #override \n\
    Ctrl <Key> minus: smaller-vt-font() \n\
    Ctrl <Key> plus: larger-vt-font() \n\
    Ctrl <Key> 0: set-vt-font(d) \n\
	Shift Ctrl <Key> C: copy-selection(CLIPBOARD) \n\
	Shift Ctrl <Key> V: insert-selection(CLIPBOARD)
xterm*saveLines: 2000

! █▓▒░ default dpi mode
Xft.dpi       : 135
Xft*autohint  : false
Xft*hintstyle : hintfull
Xft*lcdfilter : lcddefault
Xft*antialias : true
Xft*rgba      : rgb

! █▓▒░ high dpi mode
!Xft.dpi: 190
!Xft*autohint: 0
!Xft*lcdfilter: lcddefault
!Xft*hintstyle: hintfull
!Xft*hinting: 1
!Xft*antialias: 1
!Xft*rgba: rgb
